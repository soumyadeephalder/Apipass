{
    "name": "deepsoumya/apipass",
    "description": "api token auth with malty pule user",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "version": "1.0",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Deepsoumya\\Apipass\\": "src/"
        },
        "files": [
            "src/Helpers.php",
            "src/ManageToken.php"
        ]
    },
    "authors": [
        {
            "name": "deepsoumya",
            "email": "soumyadeephalder1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "lcobucci/jwt": "^4.1",
        "illuminate/database": "^9.19",
        "illuminate/auth": "^9.19",
        "illuminate/encryption": "^9.19",
        "illuminate/routing": "^9.19",
        "illuminate/http": "^9.19",
        "illuminate/support": "^9.19"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "providers": [
                "Deepsoumya\\Apipass\\ApipassServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
